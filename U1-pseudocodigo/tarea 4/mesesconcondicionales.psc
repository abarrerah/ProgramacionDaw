Proceso sin_titulo
	Escribir "Introduzca un numero para saber su mes";
	Definir n como entero;
	leer n;
	Si n=1 Entonces
		Escribir "Su mes es Enero";
	SiNo
		Si n=2 Entonces
			acciones_por_verdadero
		SiNo
			Si n=3 Entonces
				acciones_por_verdadero
			SiNo
				Si n=4 Entonces
					acciones_por_verdadero
				SiNo
					Si n=5 Entonces
						acciones_por_verdadero
					SiNo
						acciones_por_falso
					Fin Si
				Fin Si
			Fin Si
		Fin Si
	Fin Si
FinProceso
